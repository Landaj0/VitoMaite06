<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>indexServlet</servlet-name>
        <servlet-class>Servlet.indexServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ResultadoBusquedaLogueado</servlet-name>
        <servlet-class>Servlet.ResultadoBusquedaLogueadoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BusquedaLogueado</servlet-name>
        <servlet-class>Servlet.BusquedaLogueadoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>indexServlet</servlet-name>
        <url-pattern>/indexServletXXX</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ResultadoBusquedaLogueado</servlet-name>
        <url-pattern>/ResultadoBusquedaLogueadoServletXXX</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BusquedaLogueado</servlet-name>
        <url-pattern>/BusquedaLogueadoServletXXX</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
